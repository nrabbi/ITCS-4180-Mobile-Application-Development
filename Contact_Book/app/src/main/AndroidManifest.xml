<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.nrabbi.itcs4180.hw1">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.nrabbi.itcs4180.hw1.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.nrabbi.itcs4180.hw1.CreateNewContact" />
        <activity android:name="com.nrabbi.itcs4180.hw1.DisplayContacts" />
        <activity android:name="com.nrabbi.itcs4180.hw1.EditContact" />
        <activity android:name="com.nrabbi.itcs4180.hw1.DeleteContact" />
        <activity android:name="com.nrabbi.itcs4180.hw1.ViewContact">
            <intent-filter>

                <category android:name="android.intent.category.DEFAULT" />
                <action android:name="android.intent.action.VIEW" />

            </intent-filter>
        </activity>
    </application>

</manifest>